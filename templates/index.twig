{% set entry = entry %}
{% set pageContent = { title : entry.title } %}
{% include 'includes/partials/header.twig' with pageContent %}
{% for block in entry.sections.all() %}
	{% if block.type == "jumboHeader" %}
		{% set blockContent = {
			heading : block.heading,
			text : block.text,
			link : block.href,
			linkText : block.linkText,
			image : block.image.one(),
			backgroundColor : block.backgroundColor
		} %}
	{% include 'includes/sections/jumbo-header.twig' with blockContent %}
	{% elseif block.type == "featuredTestimonalSingle" %}
		{% set blockContent = {
			image : block.image.one(),
			quote : block.quote,
			person : block.person,
			backgroundColor : block.backgroundColor
		} %}
	{% include 'includes/sections/featured-testimonal-single.twig' with blockContent %}
	{% elseif block.type == "threeUp" %}
		{% set blockContent = {
			heading : block.heading,
			text : block.text,
			image1 : block.image1.one(),
			title1 : block.title1,
			text1 : block.text1,
			linkText1 : block.linkText1,
			link1 : block.link1,
			title2 : block.title2,
			text2 : block.text2,
			linkText2 : block.linkText2,
			link2 : block.link2,
			title3 : block.title3,
			text3 : block.text3,
			linkText3 : block.linkText3,
			link3 : block.link3,
			backgroundColor : block.backgroundColor
		} %}
	{% include 'includes/sections/3-up.twig' with blockContent %}
	{% elseif block.type == "featuredCompanies" %}
		{% set blockContent = {
			heading : block.heading,
			text : block.text,
			images : block.images,
			backgroundColor : block.backgroundColor
		} %}
	{% include 'includes/sections/featured-companies.twig' with blockContent %}
	{% elseif block.type == "explanationLeft" %}
		{% set blockContent = {
			heading : block.heading,
			text : block.text,
			link : block.href,
			linkText : block.linkText,
			image : block.image.one(),
			backgroundColor : block.backgroundColor
		} %}
	{% include 'includes/sections/explanation-left.twig' with blockContent %}
	{% elseif block.type == "explanationRight" %}
		{% set blockContent = {
			heading : block.heading,
			text : block.text,
			link : block.href,
			linkText : block.linkText,
			image : block.image.one(),
			backgroundColor : block.backgroundColor
		} %}
	{% include 'includes/sections/explanation-right.twig' with blockContent %}
	{% elseif block.type == "jumboCta" %}
		{% set blockContent = {
			heading : block.heading,
			text : block.text,
			link : block.href,
			linkText : block.linkText,
			backgroundColor : block.backgroundColor
		} %}
	{% include 'includes/sections/jumbo-cta.twig' with blockContent %}
	{% elseif block.type == "threeUpPeople" %}
		{% set blockContent = {
			heading : block.heading,
			text : block.text,
			image1 : block.image1.one(),
			title1 : block.title1,
			subtitle1 : block.subtitle1,
			text1 : block.text1,
			link1 : block.linkedinLink1,
			title2 : block.title2,
			subtitle2 : block.subtitle2,
			text2 : block.text2,
			link2 : block.linkedinLink2,
			title3 : block.title3,
			subtitle3 : block.subtitle3,
			text3 : block.text3,
			link3 : block.linkedinLink3,
			backgroundColor : block.backgroundColor
		} %}
	{% include 'includes/sections/3-up-people.twig' with blockContent %}
	{% elseif block.type == "accordionFaq" %}
		{% set blockContent = {
			heading : block.heading,
			text : block.text,
			faqs : block.faqs
		} %}
	{% include 'includes/sections/accordion-faq.twig' with blockContent %}
	{% elseif block.type == "textEditor" %}
		{% set blockContent = { text : block.text } %}
	{% include 'includes/sections/text-editor.twig' with blockContent %}
	{% elseif block.type == "listPricing" %}
		{% set blockContent = {
			heading : block.heading,
			text : block.text,
			pricings : block.pricings,
			baseFeatures : block.baseFeatures,
			premiumFeatures : block.premiumFeatures,
			backgroundColor : block.backgroundColor
		} %}
	{% include 'includes/sections/list-pricing.twig' with blockContent %}
	{% elseif block.type == "video" %}
		{% set blockContent = {
			embedCode : block.embedCode
		} %}
		{% include 'includes/sections/video.twig' with blockContent %}
	{% elseif block.type == "quotes" %}
		{% set blockContent = {
			heading : block.heading,
			text : block.text,
			quotes : block.quotes,
			backgroundColor : block.backgroundColor
		} %}
		{% include 'includes/sections/quotes.twig' with blockContent %}
	{% endif %}
{% endfor %}
{% include 'includes/partials/footer.twig' %}
